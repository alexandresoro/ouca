    "fastify-compress": "^3.6.1",
    "fastify-cors": "^6.0.2",
    "fastify-static": "^4.5.0",
    "fastify-websocket": "^4.0.0",
    "graphql": "^15.5.3",
    "lodash.deburr": "^4.1.0",
