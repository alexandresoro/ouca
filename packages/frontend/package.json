{
  "name": "@ou-ca/frontend",
  "version": "1.0.0",
  "private": true,
  "description": "Où-ça app UI",
  "contributors": [
    {
      "name": "Camille Carrier"
    },
    {
      "name": "Alexandre Soro"
    }
  ],
  "scripts": {
    "start": "vite",
    "build": "tsc --build tsconfig.build.json && vite build",
    "build:cloudflare": "vite --config vite.config.cf.ts build",
    "postinstall": "npm run generate-graphql",
    "serve": "vite preview",
    "test": "wireit",
    "lint": "wireit",
    "generate-graphql": "graphql-codegen --config codegen.ts"
  },
  "wireit": {
    "lint": {
      "command": "eslint .",
      "files": [
        "**/*",
        "!dist"
      ],
      "output": []
    },
    "test": {
      "command": "vitest run --passWithNoTests",
      "files": [
        "src/**",
        "tsconfig.json",
        "vite.config.*"
      ],
      "output": []
    }
  },
  "dependencies": {
    "@floating-ui/react": "0.24.1",
    "@fontsource/carter-one": "4.5.9",
    "@fontsource/nunito": "4.5.12",
    "@headlessui/react": "1.7.14",
    "@hookform/resolvers": "3.1.0",
    "@ou-ca/common": "*",
    "@sentry/integrations": "7.52.1",
    "@sentry/react": "7.52.1",
    "@sentry/tracing": "7.52.1",
    "@styled-icons/boxicons-regular": "10.47.0",
    "@styled-icons/boxicons-solid": "10.47.0",
    "@tanstack/react-query": "4.29.7",
    "@types/leaflet": "1.9.3",
    "@urql/exchange-auth": "2.1.2",
    "@urql/exchange-refocus": "1.0.2",
    "date-fns": "2.30.0",
    "graphql": "16.6.0",
    "i18next": "22.4.15",
    "i18next-http-backend": "2.2.0",
    "leaflet": "1.9.3",
    "mapbox-gl": "npm:empty-npm-package@1.0.0",
    "maplibre-gl": "2.4.0",
    "oidc-client-ts": "2.2.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.43.9",
    "react-i18next": "12.2.2",
    "react-leaflet": "4.2.1",
    "react-map-gl": "7.0.23",
    "react-oidc-context": "2.2.2",
    "react-router-dom": "6.11.1",
    "urql": "4.0.2",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "3.3.1",
    "@graphql-codegen/client-preset": "3.0.1",
    "@ou-ca/eslint-config": "*",
    "@sentry/vite-plugin": "0.7.2",
    "@tanstack/react-query-devtools": "4.29.7",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@types/node": "18.16.9",
    "@types/react": "18.2.6",
    "@types/react-dom": "18.2.4",
    "@vitejs/plugin-react": "4.0.0",
    "@vitest/coverage-c8": "0.31.0",
    "autoprefixer": "10.4.14",
    "daisyui": "2.51.6",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "postcss": "8.4.23",
    "tailwindcss": "3.3.2",
    "type-fest": "3.10.0",
    "typescript": "5.0.4",
    "vite": "4.3.6",
    "vitest": "0.31.0",
    "vitest-mock-extended": "1.1.3",
    "wireit": "0.9.5"
  },
  "engines": {
    "node": ">=16"
  }
}
