{
  "name": "@ou-ca/frontend",
  "version": "1.0.0",
  "private": true,
  "description": "Où-ça app UI",
  "contributors": [
    {
      "name": "Camille Carrier"
    },
    {
      "name": "Alexandre Soro"
    }
  ],
  "scripts": {
    "start": "vite",
    "build": "tsc --build tsconfig.build.json && vite build",
    "postinstall": "npm run generate-graphql",
    "serve": "vite preview",
    "test": "wireit",
    "lint": "wireit",
    "generate-graphql": "graphql-codegen --config codegen.ts"
  },
  "wireit": {
    "lint": {
      "command": "eslint .",
      "files": [
        "**/*"
      ],
      "output": []
    },
    "test": {
      "command": "vitest run --passWithNoTests",
      "files": [
        "src/**/*.ts",
        "tsconfig.json"
      ],
      "output": []
    }
  },
  "dependencies": {
    "@emotion/react": "11.10.5",
    "@emotion/styled": "11.10.5",
    "@fontsource/lato": "4.5.10",
    "@fontsource/yuji-hentaigana-akebono": "4.5.8",
    "@mui/icons-material": "5.11.0",
    "@mui/lab": "5.0.0-alpha.118",
    "@mui/material": "5.11.7",
    "@ou-ca/common": "*",
    "@sentry/react": "7.36.0",
    "@sentry/tracing": "7.36.0",
    "@urql/exchange-refocus": "1.0.0",
    "date-fns": "2.29.3",
    "date-fns-tz": "2.0.0",
    "graphql": "16.6.0",
    "i18next": "22.4.9",
    "i18next-http-backend": "2.1.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-helmet-async": "1.3.0",
    "react-hook-form": "7.43.0",
    "react-i18next": "12.1.5",
    "react-router-dom": "6.8.1",
    "urql": "3.0.3",
    "web-vitals": "3.0.4"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "3.0.0",
    "@graphql-codegen/client-preset": "2.0.0",
    "@ou-ca/eslint-config": "*",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "14.4.3",
    "@types/node": "18.11.19",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "@vitejs/plugin-react": "3.1.0",
    "@vitest/coverage-c8": "0.28.4",
    "eslint-plugin-react": "7.32.2",
    "typescript": "4.9.5",
    "vite": "4.1.1",
    "vitest": "0.28.4",
    "vitest-mock-extended": "1.0.9",
    "wireit": "0.9.5"
  },
  "engines": {
    "node": ">=16"
  }
}
