{
  "name": "@ou-ca/frontend",
  "version": "1.0.0",
  "private": true,
  "description": "Où-ça app UI",
  "contributors": [
    {
      "name": "Camille Carrier"
    },
    {
      "name": "Alexandre Soro"
    }
  ],
  "scripts": {
    "start": "vite",
    "build": "tsc --build tsconfig.build.json && vite build",
    "build:cloudflare": "vite --config vite.config.cf.ts build",
    "postinstall": "npm run generate-graphql",
    "serve": "vite preview",
    "test": "wireit",
    "lint": "wireit",
    "generate-graphql": "graphql-codegen --config codegen.ts"
  },
  "wireit": {
    "lint": {
      "command": "eslint .",
      "files": [
        "**/*",
        "!dist"
      ],
      "output": []
    },
    "test": {
      "command": "vitest run --passWithNoTests",
      "files": [
        "src/**",
        "tsconfig.json",
        "vite.config.*"
      ],
      "output": []
    }
  },
  "dependencies": {
    "@floating-ui/react": "0.22.3",
    "@fontsource/nunito": "4.5.12",
    "@fontsource/yuji-hentaigana-akebono": "4.5.8",
    "@headlessui/react": "1.7.13",
    "@ou-ca/common": "*",
    "@sentry/integrations": "7.46.0",
    "@sentry/react": "7.46.0",
    "@sentry/tracing": "7.46.0",
    "@styled-icons/boxicons-regular": "10.47.0",
    "@styled-icons/boxicons-solid": "10.47.0",
    "@urql/exchange-refocus": "1.0.1",
    "date-fns": "2.29.3",
    "date-fns-tz": "2.0.0",
    "graphql": "16.6.0",
    "i18next": "22.4.14",
    "i18next-http-backend": "2.2.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-helmet-async": "1.3.0",
    "react-hook-form": "7.43.9",
    "react-i18next": "12.2.0",
    "react-router-dom": "6.10.0",
    "urql": "4.0.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "3.3.0",
    "@graphql-codegen/client-preset": "2.1.1",
    "@headlessui/tailwindcss": "0.1.2",
    "@ou-ca/eslint-config": "*",
    "@sentry/vite-plugin": "0.4.0",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@types/node": "18.15.11",
    "@types/react": "18.0.33",
    "@types/react-dom": "18.0.11",
    "@vitejs/plugin-react": "3.1.0",
    "@vitest/coverage-c8": "0.29.8",
    "autoprefixer": "10.4.14",
    "daisyui": "2.51.5",
    "eslint-plugin-react": "7.32.2",
    "postcss": "8.4.21",
    "tailwindcss": "3.3.1",
    "type-fest": "3.7.2",
    "typescript": "5.0.3",
    "vite": "4.2.1",
    "vitest": "0.29.8",
    "vitest-mock-extended": "1.1.3",
    "wireit": "0.9.5"
  },
  "engines": {
    "node": ">=16"
  }
}