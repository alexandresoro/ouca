{
  "name": "@ou-ca/backend",
  "version": "1.0.0",
  "description": "Où-ça backend",
  "contributors": [
    {
      "name": "Camille Carrier"
    },
    {
      "name": "Alexandre Soro"
    }
  ],
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "npm run compile:tsc && npm run copy:assets",
    "postinstall": "npm run generate-graphql",
    "start": "npm run compile:swc -- -s && npm run copy:assets && cd dist && node main",
    "start:tsc": "npm run compile:tsc:dev && npm run copy:assets && cd dist && node main",
    "lint": "eslint .",
    "test": "jest",
    "compile:swc": "npm run --prefix ../common compile:swc -- -s && swc ./src -d dist",
    "compile:tsc": "tsc --build tsconfig.build.json",
    "compile:tsc:dev": "tsc --build tsconfig.dev.json",
    "generate-graphql": "graphql-codegen",
    "copy:assets": "node scripts/copy-graphql-schemas-to-dist.mjs",
    "umzug": "cd dist && node umzug"
  },
  "dependencies": {
    "@fastify/compress": "6.2.0",
    "@fastify/cookie": "8.3.0",
    "@fastify/cors": "8.2.0",
    "@fastify/multipart": "7.4.0",
    "@fastify/static": "6.6.1",
    "@ou-ca/common": "*",
    "csv-parse": "5.3.3",
    "csv-stringify": "6.2.3",
    "date-fns": "2.29.3",
    "dotenv": "16.0.3",
    "env-var": "7.3.0",
    "exceljs": "4.3.0",
    "fastify": "4.11.0",
    "graphql": "16.6.0",
    "jose": "4.11.2",
    "mercurius": "11.5.0",
    "pino": "8.8.0",
    "pino-pretty": "9.1.1",
    "slonik": "33.0.6",
    "slonik-interceptor-field-name-transformation": "1.6.5",
    "umzug": "3.2.1",
    "zod": "3.20.2"
  },
  "devDependencies": {
    "@graphql-codegen/add": "3.2.3",
    "@graphql-codegen/cli": "2.16.4",
    "@graphql-codegen/typescript": "2.8.7",
    "@graphql-codegen/typescript-resolvers": "2.7.12",
    "@swc/cli": "0.1.59",
    "@swc/core": "1.3.26",
    "@swc/jest": "0.2.24",
    "@types/jest": "29.2.5",
    "@types/node": "18.11.18",
    "@typescript-eslint/eslint-plugin": "5.48.1",
    "@typescript-eslint/parser": "5.48.1",
    "cpy": "9.0.1",
    "eslint": "8.31.0",
    "eslint-import-resolver-typescript": "3.5.3",
    "eslint-plugin-import": "2.27.4",
    "eslint-plugin-jest": "27.2.1",
    "jest": "29.3.1",
    "jest-mock-extended": "3.0.1",
    "type-fest": "3.5.1",
    "typescript": "4.9.4"
  },
  "engines": {
    "node": ">=18"
  }
}
