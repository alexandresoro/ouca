{
  "name": "@ou-ca/backend",
  "version": "1.0.0",
  "description": "Où-ça backend",
  "contributors": [
    {
      "name": "Camille Carrier"
    },
    {
      "name": "Alexandre Soro"
    }
  ],
  "license": "MPL-2.0-no-copyleft-exception",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "pnpm run compile:tsc",
    "start": "tsx watch -r dotenv/config src/main.ts",
    "start:tsc": "pnpm run compile:tsc:dev && node -r dotenv/config dist/main",
    "test": "find ./src -name '*.test.ts' | tr '\\n' ' ' | xargs node --import tsx --test",
    "lint": "eslint .",
    "lint:ci": "eslint .",
    "typecheck": "tsc --build tsconfig.build.json",
    "biome:ci": "biome ci *",
    "compile:tsc": "tsc --build tsconfig.build.json && tsc-alias",
    "compile:tsc:dev": "tsc --build tsconfig.dev.json && tsc-alias",
    "umzug": "node dist/umzug",
    "umzug:dev": "tsx -r dotenv/config src/umzug.ts"
  },
  "dependencies": {
    "@bull-board/api": "5.15.3",
    "@bull-board/fastify": "5.15.3",
    "@fastify/caching": "8.3.0",
    "@fastify/compress": "7.0.3",
    "@fastify/cors": "9.0.1",
    "@fastify/multipart": "8.2.0",
    "@fastify/under-pressure": "8.3.0",
    "@ou-ca/common": "workspace:*",
    "@sentry/node": "7.109.0",
    "@sentry/tracing": "7.109.0",
    "@turf/helpers": "6.5.0",
    "bullmq": "5.5.4",
    "content-disposition": "0.5.4",
    "csv-parse": "5.5.5",
    "date-fns": "3.6.0",
    "escape-string-regexp": "5.0.0",
    "exceljs": "4.4.0",
    "fastify": "4.26.2",
    "fastify-plugin": "4.5.1",
    "ioredis": "5.3.2",
    "kysely": "0.27.3",
    "nanoid": "5.0.7",
    "neverthrow": "6.1.0",
    "pg": "8.11.5",
    "pino": "8.19.0",
    "pino-pretty": "11.0.0",
    "serialize-error": "11.0.3",
    "umzug": "3.8.0",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@biomejs/biome": "1.6.4",
    "@faker-js/faker": "8.4.1",
    "@ou-ca/eslint-config": "workspace:*",
    "@types/content-disposition": "0.5.8",
    "@types/node": "20.12.5",
    "@types/pg": "8.11.4",
    "dotenv": "16.4.5",
    "fishery": "2.2.2",
    "tsc-alias": "1.8.8",
    "tsx": "4.7.2",
    "typescript": "5.4.4"
  },
  "volta": {
    "extends": "../../package.json"
  }
}
