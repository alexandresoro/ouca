{
  "name": "@ou-ca/backend",
  "version": "1.0.0",
  "description": "Où-ça backend",
  "contributors": [
    {
      "name": "Camille Carrier"
    },
    {
      "name": "Alexandre Soro"
    }
  ],
  "license": "MPL-2.0-no-copyleft-exception",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "pnpm run compile:tsc",
    "start": "tsx watch -r dotenv/config src/main.ts",
    "start:tsc": "pnpm run compile:tsc:dev && node -r dotenv/config dist/main",
    "test": "vitest run",
    "lint": "eslint .",
    "biome:ci": "biome ci *",
    "compile:tsc": "tsc --build tsconfig.build.json",
    "compile:tsc:dev": "tsc --build tsconfig.dev.json",
    "umzug": "node dist/umzug",
    "umzug:dev": "node -r dotenv/config dist/umzug"
  },
  "dependencies": {
    "@fastify/caching": "8.3.0",
    "@fastify/compress": "6.4.0",
    "@fastify/cors": "8.4.0",
    "@fastify/multipart": "8.0.0",
    "@fastify/static": "6.11.2",
    "@fastify/under-pressure": "8.3.0",
    "@ou-ca/common": "workspace:*",
    "@sentry/node": "7.74.1",
    "@sentry/tracing": "7.74.1",
    "@turf/helpers": "6.5.0",
    "bullmq": "4.12.4",
    "camelcase": "8.0.0",
    "content-disposition": "0.5.4",
    "csv-parse": "5.5.2",
    "csv-stringify": "6.4.4",
    "date-fns": "2.30.0",
    "es-main": "1.3.0",
    "exceljs": "4.3.0",
    "fastify": "4.24.2",
    "fastify-plugin": "4.5.1",
    "ioredis": "5.3.2",
    "pino": "8.16.0",
    "pino-pretty": "10.2.3",
    "serialize-error": "11.0.2",
    "slonik": "34.1.1",
    "umzug": "3.3.1",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@biomejs/biome": "1.2.2",
    "@ou-ca/eslint-config": "workspace:*",
    "@types/content-disposition": "0.5.6",
    "@types/node": "18.18.5",
    "@vitest/coverage-v8": "0.34.6",
    "dotenv": "16.3.1",
    "tsx": "3.13.0",
    "typescript": "5.2.2",
    "undici": "5.26.3",
    "vitest": "0.34.6",
    "vitest-mock-extended": "1.3.1"
  }
}
