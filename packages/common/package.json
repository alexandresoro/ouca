{
  "name": "@ou-ca/common",
  "version": "1.0.0",
  "description": "Où-ça common",
  "contributors": [
    {
      "name": "Camille Carrier"
    },
    {
      "name": "Alexandre Soro"
    }
  ],
  "license": "MIT",
  "private": true,
  "exports": {
    "./*": {
      "types": "./src/*.ts",
      "default": "./dist/*.js"
    }
  },
  "scripts": {
    "test": "wireit",
    "lint": "wireit",
    "compile:tsup": "tsup"
  },
  "wireit": {
    "lint": {
      "command": "eslint .",
      "files": [
        "**/*"
      ],
      "output": []
    },
    "test": {
      "command": "vitest run",
      "files": [
        "src/**/*.ts",
        "tsconfig.json"
      ],
      "output": []
    }
  },
  "dependencies": {
    "proj4": "2.8.1"
  },
  "devDependencies": {
    "@ou-ca/eslint-config": "*",
    "@types/proj4": "2.5.2",
    "@vitest/coverage-c8": "0.28.4",
    "tsup": "6.5.0",
    "typescript": "4.9.5",
    "vitest": "0.28.4",
    "vitest-mock-extended": "1.0.9",
    "wireit": "0.9.5"
  }
}
