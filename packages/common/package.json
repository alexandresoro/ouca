{
  "name": "@ou-ca/common",
  "version": "1.0.0",
  "description": "Où-ça common",
  "contributors": [
    {
      "name": "Camille Carrier"
    },
    {
      "name": "Alexandre Soro"
    }
  ],
  "license": "MIT",
  "private": true,
  "exports": {
    "./*": {
      "types": "./src/*.ts",
      "default": "./dist/*.js"
    }
  },
  "type": "module",
  "scripts": {
    "test": "wireit",
    "lint": "wireit",
    "compile:tsup": "wireit"
  },
  "wireit": {
    "compile:tsup": {
      "command": "tsup",
      "files": [
        "src/**",
        "tsconfig.json",
        "tsup.config.*"
      ],
      "output": [
        "dist/**"
      ]
    },
    "lint": {
      "command": "eslint .",
      "files": [
        "**",
        "!dist"
      ],
      "output": []
    },
    "test": {
      "command": "vitest run",
      "files": [
        "src/**",
        "tsconfig.json",
        "vite.config.*"
      ],
      "output": []
    }
  },
  "dependencies": {
    "proj4": "2.9.0"
  },
  "devDependencies": {
    "@ou-ca/eslint-config": "*",
    "@types/proj4": "2.5.2",
    "@vitest/coverage-c8": "0.29.7",
    "tsup": "6.7.0",
    "typescript": "5.0.2",
    "vitest": "0.29.7",
    "vitest-mock-extended": "1.1.3",
    "wireit": "0.9.5"
  }
}